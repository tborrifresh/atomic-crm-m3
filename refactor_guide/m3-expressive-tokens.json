      "labelMediumEmphasized": {
        "fontFamily": "var(--md-sys-typescale-label-medium-emphasized-font-family, Roboto Flex)",
        "fontSize": "14px",
        "fontWeight": 700,
        "lineHeight": "18px",
        "letterSpacing": "0.5px",
        "baseline": "labelMedium",
