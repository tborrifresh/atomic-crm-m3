  "shape": {
    "description": "M3 Expressive extends the shape system with 35 distinctive shapes and shape morphing capabilities. Shapes communicate brand personality and create visual hierarchy.",

    "expressiveShapeLibrary": {
      "description": "Extended shape library beyond baseline M3. These shapes are available in Material Shapes Library (Figma) and Jetpack Compose.",

      "categories": {
        "rounded": {
          "description": "Soft, approachable shapes with consistent corner radii",
          "shapes": {
            "roundedSmall": {
              "cornerRadius": "8px",
              "useCase": "Small buttons, chips, compact elements"
            },
            "roundedMedium": {
              "cornerRadius": "12px",
              "useCase": "Cards, dialogs, standard containers"
            },
            "roundedLarge": {
              "cornerRadius": "16px",
              "useCase": "Large cards, featured content"
            },
            "roundedExtraLarge": {
              "cornerRadius": "28px",
              "useCase": "Hero elements, splash screens"
            },
            "roundedFull": {
              "cornerRadius": "9999px",
              "useCase": "Pills, fully rounded buttons, avatars"
            }
          }
        },
        "squircle": {
          "description": "Superellipse shapes (continuous curvature) for premium, organic feel. Popular in iOS and modern design.",
          "shapes": {
            "squircleSmall": {
              "cornerRadius": "8px",
              "cornerSmoothing": "60%",
