          "value": "150ms",
          "cssVar": "--md-sys-motion-duration-expressive-short2",
          "useCase": "Button feedback, toggle states"
        },
        "expressiveShort3": {
          "value": "200ms",
          "cssVar": "--md-sys-motion-duration-expressive-short3",
          "useCase": "Chip selections, small reveals"
        },
        "expressiveShort4": {
          "value": "250ms",
          "cssVar": "--md-sys-motion-duration-expressive-short4",
          "useCase": "Card hover effects, menu items"
        },
        "expressiveMedium1": {
          "value": "300ms",
          "cssVar": "--md-sys-motion-duration-expressive-medium1",
          "useCase": "Component state transitions"
        },
        "expressiveMedium2": {
          "value": "400ms",
          "cssVar": "--md-sys-motion-duration-expressive-medium2",
          "useCase": "Panel expansions, drawer reveals"
        },
        "expressiveMedium3": {
          "value": "500ms",
          "cssVar": "--md-sys-motion-duration-expressive-medium3",
          "useCase": "Modal entrances, page transitions"
        },
        "expressiveMedium4": {
          "value": "600ms",
          "cssVar": "--md-sys-motion-duration-expressive-medium4",
          "useCase": "Complex shape morphs, hero animations"
        },
        "expressiveLong1": {
          "value": "700ms",
          "cssVar": "--md-sys-motion-duration-expressive-long1",
          "useCase": "Full-screen transitions"
        },
        "expressiveLong2": {
          "value": "850ms",
          "cssVar": "--md-sys-motion-duration-expressive-long2",
          "useCase": "Elaborate reveals, celebration animations"
        },
        "expressiveLong3": {
          "value": "1000ms",
          "cssVar": "--md-sys-motion-duration-expressive-long3",
          "useCase": "Onboarding sequences, storytelling"
        },
        "expressiveLong4": {
          "value": "1200ms",
          "cssVar": "--md-sys-motion-duration-expressive-long4",
          "useCase": "Maximum expression, dramatic reveals"
        }
      }
    },

    "expressiveEasing": {
      "description": "Easing curves optimized for expressive spring-like motion with natural overshoot.",

      "tokens": {
        "expressiveStandard": {
          "value": "cubic-bezier(0.34, 1.56, 0.64, 1)",
          "cssVar": "--md-sys-motion-easing-expressive-standard",
          "description": "Default expressive easing with slight overshoot",
          "useCase": "General purpose expressive animations"
        },
        "expressiveStandardAccelerate": {
          "value": "cubic-bezier(0.05, 0.7, 0.1, 1)",
          "cssVar": "--md-sys-motion-easing-expressive-standard-accelerate",
          "description": "Quick start with expressive finish",
          "useCase": "Elements entering the viewport"
        },
        "expressiveStandardDecelerate": {
          "value": "cubic-bezier(0.34, 1.56, 0.64, 1)",
