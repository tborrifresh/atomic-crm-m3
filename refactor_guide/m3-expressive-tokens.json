          "useCase": "Playful interactions, game-like feedback"
        },
        "bounce": {
          "value": "cubic-bezier(0.34, 1.8, 0.64, 1)",
          "cssVar": "--md-sys-motion-easing-bounce",
          "description": "Pronounced bounce effect",
          "useCase": "Notifications, alerts, attention-grabbing elements"
        },
        "elastic": {
          "value": "cubic-bezier(0.68, -0.6, 0.32, 1.6)",
          "cssVar": "--md-sys-motion-easing-elastic",
          "description": "Rubber band stretch and snap",
          "useCase": "Pull-to-refresh, draggable elements, playful UI"
        }
      }
    },

