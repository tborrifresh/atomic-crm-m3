{
  "$schema": "m3-expressive-tokens",
  "version": "1.0.0",
  "description": "Material Design 3 Expressive token system - supplementary tokens beyond baseline M3. Announced Google I/O 2025, rolling out with Android 16. Use alongside baseline M3 tokens for complete coverage.",
  "lastUpdated": "2025-12-16",
  "source": "m3.material.io",

  "metadata": {
    "purpose": "M3 Expressive adds dynamism, personality, and emotional resonance to Material Design 3 through enhanced typography, spring-based motion, and distinctive shapes.",
    "compatibility": "Requires baseline M3 tokens. Expressive tokens override or supplement baseline values.",
    "platforms": ["Android", "iOS", "Web", "Flutter", "Compose"],
    "llmUsageNotes": "When generating UI code, check if expressive variants are appropriate for the context. Use emphasized typography for hero content, CTAs, and emotional moments. Use expressive motion for delightful interactions. Use distinctive shapes for brand personality."
  },

  "typography": {
    "description": "M3 Expressive adds 15 emphasized typography variants alongside the 15 baseline styles. Emphasized styles use heavier weights, larger sizes, and tighter line heights to create visual impact and clearer hierarchy.",

    "emphasizedVariants": {
      "description": "Each emphasized variant corresponds to a baseline style but with increased visual weight. Use for hero content, primary CTAs, emotional moments, and key information.",

      "displayLargeEmphasized": {
        "fontFamily": "var(--md-sys-typescale-display-large-emphasized-font-family, Roboto Flex)",
        "fontSize": "64px",
        "fontWeight": 700,
        "lineHeight": "72px",
        "letterSpacing": "-0.5px",
        "baseline": "displayLarge",
        "useCase": "Hero headlines, splash screens, maximum impact moments"
      },
      "displayMediumEmphasized": {
        "fontFamily": "var(--md-sys-typescale-display-medium-emphasized-font-family, Roboto Flex)",
        "fontSize": "52px",
        "fontWeight": 700,
        "lineHeight": "58px",
        "letterSpacing": "-0.25px",
        "baseline": "displayMedium",
        "useCase": "Major section headers, landing page headlines"
      },
      "displaySmallEmphasized": {
        "fontFamily": "var(--md-sys-typescale-display-small-emphasized-font-family, Roboto Flex)",
        "fontSize": "40px",
        "fontWeight": 600,
        "lineHeight": "46px",
        "letterSpacing": "0px",
        "baseline": "displaySmall",
        "useCase": "Secondary headlines, promotional content"
      },
      "headlineLargeEmphasized": {
        "fontFamily": "var(--md-sys-typescale-headline-large-emphasized-font-family, Roboto Flex)",
        "fontSize": "36px",
        "fontWeight": 600,
        "lineHeight": "42px",
        "letterSpacing": "0px",
        "baseline": "headlineLarge",
        "useCase": "Page titles requiring emphasis, feature announcements"
      },
      "headlineMediumEmphasized": {
        "fontFamily": "var(--md-sys-typescale-headline-medium-emphasized-font-family, Roboto Flex)",
        "fontSize": "32px",
        "fontWeight": 600,
        "lineHeight": "38px",
        "letterSpacing": "0px",
        "baseline": "headlineMedium",
        "useCase": "Important section headers, modal titles"
      },
      "headlineSmallEmphasized": {
        "fontFamily": "var(--md-sys-typescale-headline-small-emphasized-font-family, Roboto Flex)",
        "fontSize": "28px",
        "fontWeight": 600,
        "lineHeight": "34px",
        "letterSpacing": "0px",
        "baseline": "headlineSmall",
        "useCase": "Card titles needing emphasis, alert headers"
      },
      "titleLargeEmphasized": {
        "fontFamily": "var(--md-sys-typescale-title-large-emphasized-font-family, Roboto Flex)",
        "fontSize": "24px",
        "fontWeight": 700,
