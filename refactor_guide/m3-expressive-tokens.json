      "labelSmallEmphasized": {
        "fontFamily": "var(--md-sys-typescale-label-small-emphasized-font-family, Roboto Flex)",
        "fontSize": "12px",
        "fontWeight": 700,
        "lineHeight": "16px",
        "letterSpacing": "0.5px",
        "baseline": "labelSmall",
        "useCase": "Badge text, status indicators, micro-labels"
      }
    },

    "variableFontAxes": {
      "description": "M3 Expressive leverages variable fonts (Roboto Flex) for dynamic typography that responds to user interaction and motion. These axes enable fluid transitions and expressive feedback.",

      "axes": {
        "wght": {
          "name": "Weight",
          "min": 100,
          "max": 1000,
          "default": 400,
          "cssProperty": "font-weight",
          "description": "Controls stroke thickness. Animate for press feedback or emphasis transitions."
        },
        "wdth": {
          "name": "Width",
          "min": 25,
          "max": 151,
          "default": 100,
          "cssProperty": "font-stretch",
          "unit": "%",
          "description": "Controls character width. Use narrower for space constraints, wider for emphasis."
        },
        "opsz": {
          "name": "Optical Size",
          "min": 8,
          "max": 144,
          "default": 14,
          "cssProperty": "font-optical-sizing",
          "description": "Automatically adjusts letterforms for size. Let browser handle or override for stylistic effect."
        },
        "GRAD": {
          "name": "Grade",
          "min": -200,
          "max": 150,
          "default": 0,
          "description": "Adjusts stroke weight without changing character width. Perfect for hover states and subtle emphasis without layout shift."
        },
        "slnt": {
          "name": "Slant",
          "min": -10,
          "max": 0,
          "default": 0,
          "unit": "deg",
          "description": "Controls slant angle. Use for italicized variants or dynamic motion feedback."
        },
        "XTRA": {
          "name": "Counter Width",
          "min": 323,
          "max": 603,
          "default": 468,
          "description": "Adjusts internal counter spaces. Fine-tune for specific optical adjustments."
        },
        "XOPQ": {
          "name": "Thick Stroke",
          "min": 27,
          "max": 175,
          "default": 96,
          "description": "Controls thickness of vertical strokes independently."
        },
        "YOPQ": {
          "name": "Thin Stroke",
          "min": 25,
          "max": 135,
          "default": 79,
          "description": "Controls thickness of horizontal strokes independently."
        },
        "YTLC": {
          "name": "Lowercase Height",
          "min": 416,
          "max": 570,
          "default": 514,
          "description": "Adjusts x-height of lowercase letters."
        },
        "YTUC": {
          "name": "Uppercase Height",
          "min": 528,
          "max": 760,
          "default": 712,
          "description": "Adjusts cap height of uppercase letters."
        },
        "YTAS": {
          "name": "Ascender Height",
          "min": 649,
          "max": 854,
          "default": 750,
          "description": "Controls height of ascenders (b, d, f, h, k, l)."
        },
        "YTDE": {
          "name": "Descender Depth",
          "min": -305,
          "max": -98,
          "default": -203,
          "description": "Controls depth of descenders (g, j, p, q, y)."
        },
        "YTFI": {
          "name": "Figure Height",
          "min": 560,
          "max": 788,
          "default": 738,
          "description": "Controls height of numerals."
        }
      },

      "expressivePresets": {
        "description": "Pre-configured axis combinations for common expressive scenarios",

        "buttonPress": {
          "description": "Subtle weight increase on button press for tactile feedback",
          "transition": "150ms ease-out",
          "from": { "wght": 500, "GRAD": 0 },
          "to": { "wght": 600, "GRAD": 50 }
        },
        "hoverEmphasis": {
          "description": "Grade increase on hover without layout shift",
          "transition": "200ms ease-in-out",
          "from": { "GRAD": 0 },
          "to": { "GRAD": 100 }
        },
        "focusExpand": {
          "description": "Slight width expansion on focus for accessibility",
          "transition": "250ms ease-out",
          "from": { "wdth": 100 },
          "to": { "wdth": 105 }
        },
        "loadingPulse": {
          "description": "Weight oscillation for loading states",
          "animation": "pulse 1.5s ease-in-out infinite",
          "keyframes": {
            "0%": { "wght": 400 },
            "50%": { "wght": 500 },
            "100%": { "wght": 400 }
          }
        },
        "heroEntrance": {
          "description": "Dynamic entrance for hero typography",
          "transition": "600ms cubic-bezier(0.34, 1.56, 0.64, 1)",
          "from": { "wght": 300, "wdth": 90, "GRAD": -100 },
          "to": { "wght": 700, "wdth": 100, "GRAD": 0 }
        }
      }
    }
  },

  "motion": {
    "description": "M3 Expressive introduces spring-based physics animations that feel natural, fluid, and responsive. The expressive motion system creates delightful interactions that respond to user intent.",

    "motionSchemes": {
      "description": "Two motion schemes define the overall feel of animations throughout an application.",

      "standard": {
        "name": "Standard Motion",
        "description": "Baseline M3 motion. Efficient, purposeful, minimal. Best for productivity apps, data-heavy interfaces, and frequent interactions.",
        "characteristics": [
          "Quick",
          "Efficient",
          "Minimal overshoot",
          "Predictable"
        ],
        "springConfig": {
          "stiffness": 400,
          "damping": 30,
          "mass": 1
        }
      },
      "expressive": {
        "name": "Expressive Motion",
        "description": "Enhanced M3 Expressive motion. Bouncy, playful, delightful. Best for consumer apps, onboarding flows, celebrations, and emotional moments.",
        "characteristics": [
          "Bouncy",
          "Playful",
          "Natural overshoot",
          "Delightful"
        ],
        "springConfig": {
          "stiffness": 300,
          "damping": 20,
          "mass": 1
        }
      }
    },

    "expressiveDuration": {
      "description": "Duration tokens for expressive animations. Generally longer than standard to allow spring physics to complete.",

      "tokens": {
